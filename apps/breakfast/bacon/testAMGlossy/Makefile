COMPONENT=TestSenderAppC

#defaults
FLOOD_INTERVAL?=5120
IS_ORIGINATOR?=FALSE
TEST_CHANNEL?=64
RETX_DELAY?=65535UL

PFLAGS += -I$(TOSDIR)/lib/serialprintf
PFLAGS += -I$(TOSDIR)/lib/rf1a-delayedSend
PFLAGS += -I$(TOSDIR)/lib/rf1a-amglossy
CFLAGS += -I../concxmit-redux
CFLAGS += -I../../util

#16mhz mclk, 32 mhz dco
PFLAGS+=-DMSP430XV2_DCO_CONFIG=MSP430XV2_DCO_32MHz_RSEL6

CFLAGS+=-DXT2_SMCLK

CFLAGS+=-DIS_ORIGINATOR=$(IS_ORIGINATOR)
CFLAGS+=-DFLOOD_INTERVAL=$(FLOOD_INTERVAL)
CFLAGS+=-DTEST_CHANNEL=$(TEST_CHANNEL)
CFLAGS+=-DRETX_DELAY=$(RETX_DELAY)

include $(MAKERULES)
